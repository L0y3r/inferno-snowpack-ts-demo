{
    "env": {
        "inferno-ts": {
            "plugins": [
                ["babel-plugin-inferno", { "imports": true }],
                ["@babel/plugin-proposal-class-properties"]
            ],
            "presets": ["@babel/preset-typescript"]
        },
        "snowpack": {
            "plugins": [
                ["snowpack/assets/babel-plugin.js", {}]
            ]
        },
        "snowpack-prod": {
            "plugins": [
                ["snowpack/assets/babel-plugin.js", {
                    "removeConsole": true,
                    "removeDebugger": true
                }]
            ],
            "presets": ["minify"]
        }
    }
}